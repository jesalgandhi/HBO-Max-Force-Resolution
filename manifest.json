{
  "name": "HBO Max â€¢ Force Resolution",
  "version": "1.2.2",
  "manifest_version": 2,
  "description": "Allows you to force 1080p HD video on HBOMax.com.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "HBO Max Resolution",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "*://*.hbomax.com/",
    "webRequestBlocking",
    "webRequest",
    "declarativeContent",
    "storage"
  ],
  "content_scripts": [{
    "matches": ["*://*.hbomax.com/*"],
    "js": ["src/inject/inject.js"]
  }],
   "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'"
}
